  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avikalpg/github-profile/issues"
  },
  "homepage": "https://github.com/avikalpg/github-profile#readme",
  "dependencies": {
    "create-react-app": "^3.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^6.6.1",
    "nodemon": "^2.0.4"
  }
}
