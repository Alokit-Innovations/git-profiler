{
  "name": "github-profile",
  "version": "1.0.0",
  "description": "A utility for creating a user's tech-profile based on the commits of the user across repositories",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "start-watch": "nodemon src/index.js",
